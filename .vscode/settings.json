{
    "files.trimTrailingWhitespace": true,
    "files.associations": {
        "*.json": "jsonc"
    },
    "files.exclude": {
        "**/.vscode": false
    },
    "editor.wordWrap": "on",
    "editor.rulers": [
        80,
        120
    ],
    "editor.renderWhitespace": "all",
    "editor.fontSize": 12,
    "editor.renderControlCharacters": true,
    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Args": [
        "max-line-length=120"
    ],
    "extensions.ignoreRecommendations": false,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "python.formatting.provider": "yapf",
    "python.formatting.yapfArgs": [
        "--style",
        "{BASED_ON_STYLE = pep8, BLANK_LINE_BEFORE_NESTED_CLASS_OR_DEF = true, SPLIT_BEFORE_EXPRESSION_AFTER_OPENING_PAREN = true, COLUMN_LIMIT = 120}"
    ],
    "git.autofetch": true,
    "editor.minimap.showSlider": "always",
    "debug.showBreakpointsInOverviewRuler": true,
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "workbench.editorAssociations": {
        "*.pdf": "latex-workshop-pdf-hook",
        "*.ipynb": "jupyter-notebook"
    },
    "git.ignoreLegacyWarning": true,
    "python.pythonPath": "C:/ProgramData/Anaconda3/envs/sr17/python.exe",
    "python.autoComplete.extraPaths": [
        "C:/ProgramData/Anaconda3/Lib/site-packages",
        "C:/ProgramData/Anaconda3/Lib",
        "C:/ProgramData/Anaconda3/pkgs"
    ],
    "gitlens.advanced.messages": {
        "suppressGitMissingWarning": true,
        "suppressGitVersionWarning": true
    },
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.watcherExclude": {
        "**/.eggs/**": true,
        "**/build/**": true,
        "**/datasets/**": true,
        "**/experiments/**/*.pth": true,
        "**/tb_logger/**": true,
